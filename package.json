{
  "name": "any-ts",
  "private": false,
  "version": "0.38.0",
  "author": {
    "name": "Andrew Jarrett",
    "email": "ahrjarrett@gmail.com",
    "url": "https://github.com/ahrjarrett"
  },
  "description": "a small set of TypeScript namespaces that overload built-in names (like any) to make them extensible",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahrjarrett/any-ts.git"
  },
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "",
  "sideEffects": "false",
  "scripts": {
    "build": "pnpm clean:build && pnpm tsup src/index.ts \"!src/**/*.spec.ts\" --dts-only",
    "check": "tsc --noEmit",
    "clean": "pnpm run \"/^check:.*/\"",
    "clean:build": "rm -rf dist",
    "clean:deps": "rm -rf dist",
    "lint": "tsc --noEmit",
    "release": "./bin/version.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.25",
    "init": "^0.1.2",
    "tsup": "^8.0.1",
    "typescript": "5.3.3"
  }
}