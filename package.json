{
  "name": "any-ts",
  "private": false,
  "version": "0.36.2",
  "author": "Andrew Jarrett <ahrjarrett@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahrjarrett/any-ts.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm clean:build && pnpm tsup src/index.ts \"!src/**/*.spec.ts\" --dts-only",
    "check": "tsc --noEmit",
    "clean": "pnpm run \"/^check:.*/\"",
    "clean:build": "rm -rf dist",
    "clean:deps": "rm -rf dist",
    "lint": "tsc --noEmit",
    "release": "./bin/version.ts",
    "release-anyway": "pnpm run release --no-git-checks"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.24",
    "init": "^0.1.2",
    "tsup": "^8.0.1",
    "typescript": "5.3.3"
  }
}